@model JdShopEx.Models.Goods
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
@{ 
    var goodDetail = (JdShopEx.Models.Goods)ViewData["GoodDetail"];
}
<head>
    <title> @goodDetail.GoodsName 详情页面</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/css/bootstrap.min.css" />
    <script src="/Content/js/jquery-1.6.4.js"></script>
    <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<style>
    .col-sm-4 img{
        width:100%;
        height:100%;
    }
</style>
<body>
    <div class="col-sm-4">
        <img src="@goodDetail.GoodsImg"/>
    </div>
    <div class="col-sm-8">
        <h3>@goodDetail.GoodsName</h3>
        <span>@goodDetail.GoodsPrice</span>
        <span>商品详细信息描述</span>
        <a href="../AddCart/@goodDetail.GoodsId">加入购物车</a>
    </div>
</body>